year = int(input("Enter the year to check: "))
# on every year that is evenly divisible by 4
if (year % 4 == 0):
    if(year % 100 == 0):
        if(year % 400 == 0):
            print("Leap year")
        else:
            print("Not leap year.")
    else:
        print(" leap year.")
else:
    print("Not leap year.")
