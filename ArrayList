
row1 = ["⬜️","️⬜️","️⬜️"]
row2 = ["⬜️","⬜️","️⬜️"]
row3 = ["⬜️️","⬜️️","⬜️️"]
map = [row1, row2, row3]
print(f"{row1}\n{row2}\n{row3}")
position = input("Where do you want to put the treasure? ")


#lets say he entered 23 - This means its colume 2 and row 3

row = int((position[1]))
row -= 1

coloumn = int((position[0]))
coloumn -= 1
map[row][coloumn] = 'X'



print(f"{row1}\n{row2}\n{row3}")
